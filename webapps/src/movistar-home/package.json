    "node-sass": "^4.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-scripts": "3.4.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
